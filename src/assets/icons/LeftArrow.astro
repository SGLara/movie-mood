---
interface Props {
    size?: number;
    className?: string;
}

const { size = 24, className = "" } = Astro.props;
---

<svg
    xmlns="http://www.w3.org/2000/svg"
    class={className}
    width={size}
    height={size}
    fill="none"
    stroke="currentColor"
    stroke-width="2"
    stroke-linecap="round"
    stroke-linejoin="round"
    class="lucide lucide-arrow-left"
>
    <path d="m12 19-7-7 7-7M19 12H5"></path>
</svg>
