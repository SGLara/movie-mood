{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/context/*": [
        "src/context/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/assets/*": [
        "src/assets/*"
      ],
      "@/api/*": [
        "src/api/*"
      ],
      "@/store/*": [
        "src/store/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/helpers/*": [
        "src/helpers/*"
      ],
      "@/config/*": [
        "src/config/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}